#sets
my_set = {1,2,3,4,5,5}
print(my_set)
my_set.add(199)
my_set.add(2)
print(my_set)

my_list = [1,2,3,4,5,5]
print(set(my_list)) # removing all diplicate data

my_set = {1,2,3,4,5,5}
print(my_set[0])

print(1 in my_set)
print(len(my_set)) # only count the unique things

new_set = my_set.copy()
my_set.clear()
print(new_set)

### functions of set
my_set = {1,2,3,4,5}
your_set = {4,5,6,7,8,9.10}

print(my-set.difference(your_set))
